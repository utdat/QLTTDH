<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
  
  <context id="DB2Tables" targetRuntime="MyBatis3">
    <jdbcConnection driverClass="com.mysql.cj.jdbc.Driver"
        connectionURL="jdbc:mysql://127.0.0.1:3306/qltt"
        userId="root"
        password="">
    </jdbcConnection>

    <javaTypeResolver >
      <property name="forceBigDecimals" value="false" />
    </javaTypeResolver>

    <javaModelGenerator targetPackage="com.quanlytrungtamdayhoc.dbo" targetProject="QLTT" />

    <sqlMapGenerator targetPackage="com.quanlytrungtamdayhoc.mapper.sql"  targetProject="QLTT/src/main/resources" />
    
    <javaClientGenerator type="XMLMAPPER" targetPackage="com.quanlytrungtamdayhoc.mapper"  targetProject="QLTT" />
    
    <table tableName="accounts" domainObjectName="Account" >

    </table>
    
    <table tableName="announcements" domainObjectName="Announcement" >

    </table>
    
    <table tableName="salary" domainObjectName="Salary" >

    </table>
    
    <table tableName="students" domainObjectName="Student" >

    </table>
    
    <table tableName="student_scores" domainObjectName="Student_score" >

    </table>
    
    <table tableName="subjects" domainObjectName="Subject" >

    </table>
    
    <table tableName="submit_homework" domainObjectName="Submit_homework" >

    </table>
    
    <table tableName="teachers" domainObjectName="Teacher" >

    </table>
    
    <table tableName="tuition" domainObjectName="Tuition" >

    </table>

  </context>
</generatorConfiguration>